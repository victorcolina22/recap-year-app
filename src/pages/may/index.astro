---
import Layout from "../../layouts/Layout.astro";
import NavigationClick from "../../components/NavigationClick.astro";
import Container from "../../components/Container.astro";
---

<Layout title="May">
  <NavigationClick previous="/april-photos" next="/may-photos" />
  <Container>
    <section class="grid grid-cols-1 grid-rows-1 place-items-center items-end">
      <div class="z-20 mt-5">
        <h2 class="text-[32px] text-center animate-fadeDown -mb-4 -ml-10">
          I got
        </h2>
        <h2 class="text-[62px] text-center animate-fadeUp">
          How <span class="line-through">you get</span> the girl
        </h2>
      </div>

      <div class="absolute left-0 right-0 bottom-0 grid place-items-center">
        <img src="images/may/Group 86.png" alt="group 86" />
      </div>
    </section>
  </Container>
</Layout>
